   "source": [
    "from vtkplotter import *\n",
    "\n",
    "embedWindow(backend='panel')\n",
    "\n",
    "vp = Plotter()\n",
    "\n",
    "c = Sphere()\n",
    "scals = c.coordinates()[:,2]\n",
    "c.pointColors(scals) # use z-coords to color vertices\n",
    "vp += c # add mesh to Plotter list\n",
    "\n",
    "c.printInfo()\n",
    "vp.show()"
   ]
